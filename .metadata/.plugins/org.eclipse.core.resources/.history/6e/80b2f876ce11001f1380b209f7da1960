		Scanner sc = new Scanner(System.in);
		
		System.out.println("가로의 길이: ");
		int A = sc.nextInt();
		System.out.println("세로의 길이: ");
		int B = sc.nextInt();
		
		System.out.println(showArea(A, B));
	}
}
