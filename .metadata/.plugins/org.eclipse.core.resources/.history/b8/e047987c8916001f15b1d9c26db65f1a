package prac_main;

import java.util.Scanner;

import prac_Phone.Android;
import prac_Phone.IPhone;

public class Main{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		IPhone I = new IPhone();
		Android A = new Android();
		
		System.out.println("핸드폰 기종이 무엇인가요 : ");
		String p_name = sc.next();
		
		I.iphone(p_name);
		A.android(p_name);
	}
}
