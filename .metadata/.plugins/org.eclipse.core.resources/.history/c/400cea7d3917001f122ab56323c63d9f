package test.main;

import test.mypac.Weapon;

public class MainClass05 {
	static class SeaWeapon extends Weapon{

		@Override
		public void attack() {
			System.out.println("바다에 있는 적들을 공격해요!");
			
		}
		
	}
	public static void main(String[] args) {
		Weapon w1 = new SeaWeapon();
		w1.attack();
	}
}
