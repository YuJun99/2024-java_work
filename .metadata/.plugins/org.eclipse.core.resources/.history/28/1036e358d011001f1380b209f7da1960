package test.main;

import java.util.Scanner;
import test.mypac.QuadArea;

public class Quad {
	public static void main(String[] args) {
		QuadArea num1 = new QuadArea();
		QuadArea num2 = new QuadArea();
		
		num1.width = 10;
		num1.height = 10;
		
		num2.width = 20;
		num2.height = 30;
		
		num1.showArea();
		num2.showArea();
	}
}
